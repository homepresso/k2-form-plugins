    {
      "id": "lineHeight",
      "refreshdisplay": "true",
      "friendlyname": "Line Height",
      "description": "Line height (e.g., 1.5, 24px, normal)",
      "type": "string",
      "inputlength": "20",
      "initialvalue": "1.5",
      "changesdisplay": true
    },
    {
      "id": "textDecoration",
      "refreshdisplay": "true",
      "friendlyname": "Text Decoration",
      "description": "Text decoration: none, underline, line-through",
      "type": "string",
      "inputlength": "20",
      "initialvalue": "none",
      "changesdisplay": true
    },
    {
      "id": "textTransform",
      "refreshdisplay": "true",
      "friendlyname": "Text Transform",
      "description": "Text transform: none, uppercase, lowercase, capitalize",
      "type": "string",
      "inputlength": "20",
      "initialvalue": "none",
      "changesdisplay": true
    },
    {
      "id": "wordWrap",
      "refreshdisplay": "true",
      "friendlyname": "Word Wrap",
      "description": "Allow text to wrap (true/false)",
      "type": "string",
      "inputlength": "10",
      "initialvalue": "true",
      "changesdisplay": true
