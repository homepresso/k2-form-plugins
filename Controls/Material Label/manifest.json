    {
      "id": "clickable",
      "refreshdisplay": "true",
      "friendlyname": "Clickable",
      "description": "Make the label clickable (true/false)",
      "type": "string",
      "inputlength": "10",
      "initialvalue": "false",
      "changesdisplay": true
