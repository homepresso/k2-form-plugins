    "Value",
    "Width",
    "IsVisible",
    "IsEnabled",
    "IsReadOnly"
  ],
  "events": [
    {
      "id": "Changed",
      "description": "Fires when the textbox value changes"
    },
    {
      "id": "Focus",
      "description": "Fires when the textbox receives focus"
    },
    {
      "id": "Blur",
      "description": "Fires when the textbox loses focus"
    },
    {
      "id": "OnEnter",
      "description": "Fires when Enter key is pressed"
    },
    {
      "id": "IconClicked",
      "description": "Fires when the icon is clicked"
    }
